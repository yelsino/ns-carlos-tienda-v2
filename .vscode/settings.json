{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "files.exclude": { 
    "**/node_modules": true, 
    "**/.firebase": true,
    "**/.jest": true,
    "**/.github": true,
   },
   "workbench.colorCustomizations": {
    "statusBar.noFolderBackground": "#ffff"
   }
}
